{
  "name": "devs.coffee-back-end",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "set DEBUG=express:* & nodemon --watch ./** --ext ts,json --ignore ./**/*.spec.ts --exec ts-node --project ./tsconfig.json ./bin/www.ts",
    "start": "node ./bin/www.ts",
    "lint": "eslint ./**/*.ts",
    "lint-fix": "eslint --fix ./**/*.ts",
    "format": "prettier --write ./**/*.ts"
  },
  "dependencies": {
    "@types/cors": "^2.8.9",
    "@types/mongoose": "^5.10.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "mongoose": "^5.11.14",
    "morgan": "^1.10.0",
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
